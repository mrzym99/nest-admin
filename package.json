{
  "name": "nest-admin",
  "version": "1.0.0",
  "description": "A backend management system developed based on Nest, with a technology stack including NestJS typeorm、mysql、redis、typescript",
  "author": {
    "name": "XiaoZhang",
    "email": "2715158815@qq.com",
    "url": "https://github.com/mrzym99/nest-admin"
  },
  "license": "MIT",
  "homepage": "https://github.com/mrzym99/nest-admin",
  "repository": {
    "url": "https://github.com/mrzym99/nest-admin.git"
  },
  "bugs": {
    "url": "https://github.com/mrzym99/nest-admin/issues"
  },
  "keywords": [
    "nest admin",
    "typeorm",
    "mysql",
    "nest",
    "nestjs",
    "admin",
    "typescript",
    "redis"
  ],
  "engines": {
    "node": ">=18.12.0",
    "pnpm": ">=8.7.0"
  },
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:hot": "nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "NODE_ENV=development typeorm-ts-node-commonjs -d ./dist/config/database.config.js",
    "migration:create": "typeorm-ts-node-commonjs migration:create ./src/migrations",
    "migration:generate": "npm run typeorm migration:generate ./src/migrations/update-table_$(echo $npm_package_version | sed 's/\\./_/g')",
    "migration:run": "npm run typeorm -- migration:run",
    "migration:revert": "npm run typeorm -- migration:revert",
    "docker:build:dev": "docker-compose build",
    "docker:up:dev": "docker-compose up -d",
    "docker:build:prod": "docker-compose -f docker-compose.prod.yml build",
    "docker:up:prod": "docker-compose -f docker-compose.prod.yml up -d",
    "docker:down": "docker-compose down",
    "docker:down:v": "docker-compose down -v"
  },
  "dependencies": {
    "@fastify/cookie": "^9.4.0",
    "@fastify/multipart": "^8.3.1",
    "@fastify/static": "^7.0.4",
    "@keyv/redis": "^4.3.3",
    "@liaoliaots/nestjs-redis": "^10.0.0",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/bull": "^11.0.2",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^10.4.17",
    "@nestjs/core": "^10.4.17",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-fastify": "^10.4.17",
    "@nestjs/platform-socket.io": "^11.1.0",
    "@nestjs/swagger": "^8.1.1",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/typeorm": "^10.0.2",
    "@nestjs/websockets": "^11.1.0",
    "@socket.io/redis-adapter": "^8.3.0",
    "@socket.io/redis-emitter": "^5.1.0",
    "@types/ali-oss": "^6.16.11",
    "ali-oss": "^6.22.0",
    "alipay-sdk": "^4.13.0",
    "argon2": "^0.41.1",
    "axios": "^1.8.4",
    "bull": "^4.16.5",
    "cache-manager": "^6.4.2",
    "cacheable": "^1.8.10",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "config": "^3.3.12",
    "cos-nodejs-sdk-v5": "^2.14.7",
    "cron-parser": "^5.1.1",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.13",
    "dotenv": "^16.5.0",
    "fastify": "^4.29.0",
    "https": "^1.0.0",
    "ioredis": "^5.6.1",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "multer": "1.4.5-lts.1",
    "mysql2": "^3.14.0",
    "nest-winston": "^1.10.2",
    "nestjs-cls": "^5.4.2",
    "node-random-name": "^1.0.1",
    "nodemailer": "^6.10.1",
    "passport-jwt": "^4.0.1",
    "punycode": "^2.3.1",
    "qs": "^6.14.0",
    "reflect-metadata": "^0.2.2",
    "request-ip": "^3.3.0",
    "rxjs": "^7.8.2",
    "socket.io": "^4.8.1",
    "svg-captcha": "^1.4.0",
    "systeminformation": "^5.25.11",
    "typeorm": "^0.3.22",
    "ua-parser-js": "^2.0.3",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@nestjs/axios": "^4.0.0",
    "@nestjs/cli": "^10.4.9",
    "@nestjs/config": "^3.3.0",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/schedule": "^5.0.1",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^10.4.17",
    "@types/jest": "^29.5.14",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.17.30",
    "@types/nodemailer": "^6.4.17",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.30.1",
    "@typescript-eslint/parser": "^8.30.1",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.6",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "run-script-webpack-plugin": "^0.2.3",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.0",
    "ts-jest": "^29.3.2",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3",
    "webpack": "^5.99.5",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
